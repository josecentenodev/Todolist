<%- include('header'); -%>

    <body>
        <div class="box" id="heading">
            <h1>
                <%= listTitle %>
            </h1>
        </div>

        <div class="box">
            <% itemList.forEach(item => { %>
                <div class="item">
                    <input type="checkbox">
                    <p>
                        <%= item %>
                    </p>
                </div>
                <% }); %>

                    <form class="item" action="/" method="POST">
                        <input type="text" name="newItem" placeholder="New Item" autocomplete="off">

                        <button type="submit" name="button">+</button>
                    </form>
        </div>

    </body>
    <%- include('footer'); -%>

        </html>